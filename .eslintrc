{
  // I want to use babel-eslint for parsing!
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "env": {
    "browser": false,
    "commonjs": true,
    "amd": true,
    "es6": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "arrow-parens" : [1, "always"],
    "arrow-body-style" : 0,
    "array-bracket-spacing" : 1,
    "array-callback-return" : 1,
    "block-scoped-var" : 1,
    "block-spacing" : 1,
    "brace-style" : 1,

    "camelcase" : 0,
    "consistent-return" : 1,
    "class-methods-use-this" : 0,
    "comma-dangle" : 1,
    "comma-spacing" : 1,
    "curly" : 1,
    "default-case" : 1,
    "dot-notation" : 1,
    "eol-last" : 1,
    "func-call-spacing" : 1,
    "func-names" : 1,
    "global-require" : 0,
    "guard-for-in" : 1,

    "indent" : [1, 4],
    "import/no-extraneous-dependencies" : 0,
    "import/extensions" : 0,
    "import/first" : 1,
    "import/newline-after-import" : 1,
    "import/no-duplicates" : 1,
    "import/no-unresolved" : 0,
    "import/no-mutable-exports" : 0,
    "import/no-named-as-default" : 1,
    "import/no-named-default" : 1,
    "import/prefer-default-export" : 1,
    "key-spacing" : 1,
    "keyword-spacing" : 1,
    "max-len" : 1,
    "newline-per-chained-call" : 1,
    "new-cap" : 1,
    "new-parens" : 1,
    "no-bitwise" : 1,
    "no-continue" : 1,
    "no-cond-assign" : 1,
    "no-multiple-empty-lines" : 1,
    "no-param-reassign" : 1,
    "no-confusing-arrow" : 1,
    "no-else-return" : 1,
    "no-extra-semi" : 1,
    "no-lonely-if" : 1,
    "no-loop-func" : 1,
    "no-multi-assign" : 1,
    "no-multi-spaces" : 1,
    "no-nested-ternary" : 1,
    "no-trailing-spaces" : 1,
    "no-plusplus" : [1, { "allowForLoopAfterthoughts": true }],
    "no-prototype-builtins" : 1,
    "no-restricted-syntax" : 1,
    "no-return-assign" : 1,
    "no-sequences" : 1,
    "no-shadow" : 1,
    "no-spaced-func" : 1,
    "no-tabs" : 1,
    "no-throw-literal" : 1,
    "no-undef" : 1,
    "no-underscore-dangle" : 0,
    "no-unneeded-ternary" : 1,
    "no-unused-expressions" : 0,
    "no-unused-vars" : 1,
    "no-use-before-define" : 1,
    "no-useless-concat" : 1,
    "no-useless-rename" : 1,
    "no-useless-return" : 1,
    "no-useless-escape" : 1,
    "no-whitespace-before-property" : 1,
    "no-var" : 1,
    "no-mixed-operators" : 1,
    "no-mixed-spaces-and-tabs" : 1,
    "object-curly-spacing" : 1,
    "object-property-newline" : 1,
    "object-shorthand" : 1,
    "one-var" : 1,
    "one-var-declaration-per-line" : 1,
    "operator-assignment" : 1,
    "padded-blocks" : 1,
    "prefer-const" : 1,
    "prefer-rest-params" : 1,
    "prefer-spread" : 1,
    "prefer-template" : 1,
    "prefer-arrow-callback" : 1,
    "quotes" : 1,
    "quote-props" : 1,
    "radix" : 1,
    "semi" : 1,
    "semi-spacing" : 1,
    "space-before-blocks" : 1,
    "space-before-function-paren" : 1,
    "space-in-parens" : 1,
    "space-infix-ops" : 1,
    "space-unary-ops" : 1,
    "spaced-comment" : 1,
    "strict" : 1,
    "vars-on-top" : 1,
    "wrap-iife" : 1,
    "yoda" : 1
  }
}

